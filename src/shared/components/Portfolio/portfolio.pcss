.portfolio {
  h2 {
    color: var(--lightseagreen);
  }

  .image_block_listing {
    display: flex;
  }

  .image_block {
    display: flex;
    align-items: center;
    justify-content: center;

    background: white;
    color: black;
    flex: 0 1 200px;
    height: 150px;
    margin-right: 30px;
  }

  .image_block_overlay {
    padding: 26px 54px;
    background: black;
    color: white;
  }
}
